# Application Configuration
REFERENCE_ENV=test33
LOG_LEVEL=INFO
IN_CLUSTER=false
NAMESPACE_PREFIX=test
NAMESPACE_LABEL_KEY=createdBy
NAMESPACE_LABEL_VALUE=koderover
EXCLUDED_NAMESPACES=test17,test33

# Feature Flags
ENABLE_CERT_MANAGEMENT=true
ENABLE_DNS_MANAGEMENT=true
ENABLE_AWS_RESOURCES=true
ENABLE_KONG_ROUTES=true
ENABLE_APOLLO_CONFIG=true
ENABLE_ZADIG_WORKFLOW=true

# AWS Configuration
AWS_REGION=ap-southeast-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# MySQL Configuration
MYSQL_HOST=192.168.0.50
MYSQL_PORT=3306
MYSQL_USER=apollo_user
MYSQL_PASSWORD=your_mysql_password
MYSQL_DATABASE=ApolloConfigDB_fat

# Kong Configuration
KONG_ADMIN_URL=http://kong-kong-admin.proxy:8001

# Zadig Configuration
ZADIG_URL=http://zadigx.shub.us
ZADIG_TOKEN=your_zadig_jwt_token
ZADIG_PROJECT_KEY=fat-base-envrionment
ZADIG_GIT_REPO_OWNER=your-git-org
ZADIG_GIT_REPO_NAME_TEMPLATE={namespace_name} # e.g., service-{namespace_name} or {namespace_name}-repo
ZADIG_GIT_SERVICE_NAME_TEMPLATE={namespace_name} # e.g., {namespace_name}-svc
ZADIG_GIT_TRIGGER_EVENTS=push
ZADIG_DEFAULT_GIT_BRANCH=main

# DNS Made Easy Configuration

# DNS Made Easy Configuration
DNS_API_KEY=your_dns_api_key
DNS_SECRET_KEY=your_dns_secret_key

# cert-manager Configuration
CERT_MANAGER_NAMESPACE=cert-manager
CERT_MANAGER_ISSUER=letsencrypt-dnsmadeeasy-prod
CERT_MANAGER_ISSUER_KIND=ClusterIssuer

# Redis Configuration
REDIS_URL=redis://localhost:6379/0
REDIS_KEY_PREFIX=namespace-watcher
REDIS_TTL_DAYS=30